@import "../public/menu/menu.wxss";
.search {
  display: block;
  margin: 0px;
}

.search .col {
  display: flex;
  font-family: -apple-system-font, "Helvetica Neue", sans-serif;
  font-size: 34rpx;
}

.search input{
  border-bottom: 1px solid rgb(192,192,192);
}
.search .right{
  text-align: right;
  font-weight: 700;
}


.search .left {
  width: 75%;
}
.search .right {
  width: 25%;
}


.goods {
  display: block;
  margin: 0px;
  width: 640rpx;
  margin-top:100rpx; 
}

.goods .text{
  text-align: center;
  font-size: 24rpx;
  font-weight: 700;
}

.goods .two {
  display: flex;
  font-family: -apple-system-font, "Helvetica Neue", sans-serif;
  font-size: 34rpx;
  margin-top: 80rpx;
}

.goods .two image{
  width: 300rpx;
}

